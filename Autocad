# Crea la carpeta C:\CAD
New-Item -Path "c:\" -Name "CAD" -ItemType "directory"
# Extrae el contenido del Exe a la carpeta C:\CAD y le pausa el script para esperar que termine
powershell -Command {\\10.76.16.27\Software\04_SalasPC\Autodesk\01_AutoCAD\AAutoCAD_2017_English_Win_64bit_dlm_001_002.sfx.exe -suppresslaunch -d "C:\CAD"}
Start-Sleep -s 240
# Extrae el contenido del Exe a la carpeta C:\CAD
powershell -Command {C:\CAD\AutoCAD_2021_English_Win_64bit_dlm\Setup.exe /W /Q /I .\setup.ini}
