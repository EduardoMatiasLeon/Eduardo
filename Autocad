# Crea la carpeta C:\CAD
New-Item -Path "c:\" -Name "CAD" -ItemType "directory"
# Extrae el contenido del Exe a la carpeta C:\CAD y le pausa el script para esperar que termine
powershell -Command {C:\Users\Administrator\Desktop\Programas\10AutoCad\Setup.exe -suppresslaunch -d "C:\CAD"}
Start-Sleep -s 240
# Extrae el contenido del Exe a la carpeta C:\CAD
powershell -Command {C:\Users\Administrator\Desktop\Programas\10AutoCad\Setup.exe /W /Q /I .\setup.ini}
